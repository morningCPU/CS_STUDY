import numpy as np

a = np.array([[1, 2, 3], [4, 5, 6]])
print("a原本的值为:\n{}".format(a))
b = np.array(a)
c = np.asarray(a)
a[0][0] = 5
print("a的值为:\n{}".format(a))
print("b的值为:\n{}".format(b))  # b不会被改动
print("c的值为:\n{}".format(c))  # c 会被改动











